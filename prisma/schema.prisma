// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_SUPABASE_URL") // uses connection pooling
  directUrl = env("POSTGRES_SUPABASE_URL_NON_POOLING") // uses a direct connection
}

model User {
  id Int @id @default(autoincrement())
  userId String @unique
  fullname String
  email String @unique
  location String
  eventId String
}

model Events {
  id Int @id @default(autoincrement())
  eventId String @unique
  title String
  date String
  description String
}